word1 = input("What is the first word:")
word2 = input("What is the second word:")

word1t = word1.upper()
word2t = word2.upper()

if (word1t == word2t):
	print("The words are the same")
elif (word1t <word2t):
	print("The first word is before the second alphabetically")
else: 
	print("The second word is before the first alphabetically")